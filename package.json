{
  "name": "heroic-ui-library",
  "version": "0.9.0",
  "license": "MIT",
  "author": "Muhammad Yasir",
  "description": "React component library for Heroic Game Day.",
  "main": "dist/index.js",
  "outputDir": "./dist",
  "homepage": "https://github.com/yasirshahzad/heroic-ui-library.git#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/yasirshahzad/heroic-ui-library.git"
  },
  "scripts": {
    "start": "storybook dev -p 6006",
    "build:lib": "rollup -c --bundleConfigAsCjs",
    "build:storybook": "yarn storybook build",
    "build:publish": "yarn build && yarn publish",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "prebuild": "rimraf dist",
    "prepare": "husky install",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:pre:alpha": "standard-version --prerelease alpha",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "predeploy": "yarn build:storybook",
    "deploy": "gh-pages -d storybook-static",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-image": "^3.0.2",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-typescript": "^11.1.3",
    "@storybook/addon-actions": "^7.4.0",
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-interactions": "7.4.0",
    "@storybook/addon-links": "7.4.0",
    "@storybook/addon-onboarding": "1.0.8",
    "@storybook/addon-storysource": "^7.4.0",
    "@storybook/blocks": "7.4.0",
    "@storybook/react": "7.4.0",
    "@storybook/react-vite": "7.4.0",
    "@storybook/react-webpack5": "7.4.0",
    "@storybook/testing-library": "^0.2.1-next.0",
    "@storybook/theming": "^7.4.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/classnames": "^2.3.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@vitest/coverage-c8": "^0.25.3",
    "babel-loader": "^9.1.0",
    "classnames": "^2.3.2",
    "cross-env": "7.0.3",
    "css-loader": "^6.7.2",
    "eslint": "^8.45.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-storybook": "^0.6.13",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.2",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.1.0",
    "npmlog": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "3.0.2",
    "rollup": "^3.29.1",
    "rollup-plugin-external-assets": "^4.0.0",
    "rollup-plugin-external-globals": "^0.8.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-styles": "^4.0.0",
    "rollup-plugin-typescript-paths": "^1.4.0",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "standard-version": "^9.5.0",
    "storybook": "7.4.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.4",
    "tslib": "2.4.1",
    "typescript": "^5.0.2",
    "vite": "^3.2.5",
    "vite-tsconfig-paths": "^3.6.0",
    "vitest": "^0.25.3"
  },
  "peerDependencies": {
    "react": ">=18.x",
    "react-dom": ">=18.x"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,html}": "prettier --write"
  },
  "publishConfig": {
    "access": "public"
  },
  "resolutions": {
    "autoprefixer": "^10.4.12"
  }
}
